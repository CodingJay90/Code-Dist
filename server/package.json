{
  "name": "code-dist--server",
  "version": "1.0.0",
  "description": "backend for code-dist",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev-watch": "tsc-watch --onSuccess \"node ./dist/index.js\"",
    "dev": "tsc -w & nodemon -q -w dist dist/index.js",
    "build": "npm run compiler",
    "babel-build": "babel --extensions .ts ./source -d dist",
    "postinstall": "npm run build",
    "clean": "rimraf esm",
    "compiler": "npm run clean && tsc --pretty && babel --extensions '.ts' source -d esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodingJay90/Code-Dist.git"
  },
  "author": "codingjay90",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CodingJay90/Code-Dist/issues"
  },
  "homepage": "https://github.com/CodingJay90/Code-Dist#readme",
  "dependencies": {
    "adm-zip": "^0.5.9",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "envalid": "^7.3.1",
    "express": "^4.18.1",
    "express-healthcheck": "^0.1.0",
    "fs-extra": "^10.1.0",
    "fs-jetpack": "^4.3.1",
    "helmet": "^5.1.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^6.5.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^3.3.4",
    "unzipper": "^0.10.11",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@types/adm-zip": "^0.5.0",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.8",
    "@types/unzipper": "^0.10.5",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4"
  },
  "_moduleAliases": {
    "@/config": "dist/config",
    "@/resources": "dist/resources",
    "@/middleware": "dist/middleware",
    "@/types": "dist/utils/interfaces",
    "@/utils": "dist/utils"
  }
}
